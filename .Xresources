!! Colorscheme

! special
*.foreground:   #b8b8b8
*.background:   #000000
*.cursorColor:  #b8b8b8

! black
*.color0:       #000000
*.color8:       #434d53

! red
*.color1:       #d12f2c
*.color9:       #fa3935

! green
*.color2:       #819400
*.color10:      #a4bd00

! yellow
*.color3:       #b08500
*.color11:      #d9a400

! blue
*.color4:       #2587cc
*.color12:      #2ca2f5

! magenta
*.color5:       #087307
*.color13:      #8086e8

! cyan
*.color6:       #289c93
*.color14:      #33c5ba

! white
*.color7:       #bfbaac
*.color15:      #fdf6e3

!! Extensions:
URxvt.perl-ext-common: matcher,keyboard-select,url-select,clipboard

!! url-select:
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

!! matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.clipboard.autocopy: true

!! URxvt Appearance
URxvt.font: xft:Consolas:style=Regular:size=9 
URxvt.boldFont: xft:Consolas:style=Bold:size=9
URxvt.italicFont: xft:Consolas:style=Bold:size=9 
URxvt.boldItalicfont: xft:Consolas:style=Bold Italic:size=9
URxvt.lineSpace: 0
URxvt.internalBorder: 10 
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: false
URxvt.depth: 24

!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D
URxvt.underlineURLs: true
